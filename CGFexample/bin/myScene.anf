		<node id="anel"> 
			<transforms> 
				<transform type="rotate" axis="x" angle="20" />
				<transform type="scale" factor="1 1 0.05"/>
			</transforms> 

			<appearanceref id="inherit"/> 
			<primitives> 
				<torus inner="0.2" outer="1.5" slices="25" loops="25"/> 
			</primitives> 
		</node>
		<node id="sphere">

			<transforms>
			</transforms>

			<appearanceref id="inherit" />

			<primitives>
				<sphere radius="1" slices="40" stacks="40" />
			</primitives>

			<descendants>

			</descendants>
		</node>

		

	</graph>
</anf>

